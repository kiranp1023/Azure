{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sai-ADF1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlserver'"
		},
		"azureTable2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azureTable2'"
		},
		"azuresqltable2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuresqltable2'"
		},
		"localSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'localSQL'"
		},
		"localSQL2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'localSQL2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sai1023datalakegen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sai-ADF1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlserver_connectionString": {
						"value": "[parameters('AzureSqlserver_connectionString')]"
					},
					"azureTable2_connectionString": {
						"value": "[parameters('azureTable2_connectionString')]"
					},
					"azuresqltable2_connectionString": {
						"value": "[parameters('azuresqltable2_connectionString')]"
					},
					"localSQL_connectionString": {
						"value": "[parameters('localSQL_connectionString')]"
					},
					"localSQL2_connectionString": {
						"value": "[parameters('localSQL2_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}